//Code:
// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    int n;
    cout<<"Enter n : ";
    cin>>n;
    int arr[n][n];
    cout<<"Enter Array : "<<endl;
    for(int i=0; i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            cin>>arr[i][j];
        }
    }
    int count = 0;
    for(int i=0; i<n; i++)
    {
        for(int j=0; j<n;j++)
        {
            if(j == count)
            {
                cout<<arr[i][j];
            }
        }
        count++;
    }
    return 0;
}
